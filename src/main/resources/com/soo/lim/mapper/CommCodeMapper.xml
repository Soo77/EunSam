<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soo.lim.dao.CommCodeDao">

  <!-- 컬럼 이름과 자바 객체의 프로퍼티 이름을 연결한다. -->
  <resultMap type="CommCode" id="CommCodeMap">
    <id column="groupcode_id" property="groupcodeId"/>
    <result column="code_id" property="codeId"/>
    <result column="code_name" property="codeName"/>
  </resultMap>


<select id="findAll" resultMap="CommCodeMap">
select * from comm_code
</select>

</mapper>